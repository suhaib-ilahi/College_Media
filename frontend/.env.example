# College Media - Environment Variables Template
# Copy this file to .env.local and fill in your values

# Application Settings
VITE_APP_NAME=College Media
VITE_APP_VERSION=0.0.0
VITE_ENV=development

# API Configuration (For future backend integration)
VITE_API_TIMEOUT=10000

# Authentication
VITE_AUTH_ENABLED=false
VITE_JWT_TOKEN_KEY=auth_token

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false

# Logging
VITE_LOG_LEVEL=debug
VITE_DEBUG_MODE=false

# Media & Storage (For future implementation)
VITE_IMAGE_UPLOAD_ENDPOINT=http://localhost:3000/upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp,image/gif

# Third-party Services (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Development Settings
VITE_PORT=5173
VITE_HOST=localhost
# Frontend Environment Variables

# API Base URL (for future backend integration)
VITE_API_BASE_URL=http://localhost:5001

# Environment
VITE_ENV=development
DATABASE_URL=postgresql://user:password@localhost:5432/college_media
